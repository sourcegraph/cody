      ],
      "comments/commentThread/context": [
        {
          "command": "cody.comment.add",
          "group": "inline",
          "when": "cody.activated && commentController =~ /^cody-inline/ && config.cody.inlineChat.enabled"
        },
        {
          "command": "cody.focus",
          "group": "inline",
          "when": "!cody.activated && commentController =~ /^cody-inline/ && config.cody.inlineChat.enabled"
        }
      ],
      "comments/commentThread/title": [
        {
          "command": "cody.comment.delete",
          "group": "inline@1",
